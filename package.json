{
  "version": "2.0.2",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/qlaffont/rosetty-solid.git"
  },
  "bugs": {
    "url": "https://github.com/qlaffont/rosetty-solid/issues",
    "email": "contact@qlaffont.com"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsup src/index.tsx --external=date-fns,rosetty,solid-js",
    "test": "jest",
    "lint": "eslint src test",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rosetty-solid",
  "author": "Quentin Laffont",
  "module": "./dist/index.js",
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "300 KB"
    },
    {
      "path": "dist/index.cjs",
      "limit": "200 KB"
    }
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "babelConfig": {
          "presets": [
            "babel-preset-solid",
            "@babel/preset-env"
          ]
        }
      }
    },
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "solid-js/web": "<rootDir>/node_modules/solid-js/web/dist/web.cjs",
      "solid-js": "<rootDir>/node_modules/solid-js/dist/solid.cjs"
    }
  },
  "resolutions": {
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "11.0.2",
    "@solidjs/testing-library": "^0.8.5",
    "@testing-library/jest-dom": "6.3.0",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "babel-jest": "^29.7.0",
    "babel-preset-solid": "^1.8.6",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "np": "9.2.0",
    "prettier": "3.2.4",
    "size-limit": "11.0.2",
    "solid-jest": "^0.2.0",
    "ts-jest": "^29.1.1",
    "tslib": "2.6.2",
    "tsup": "^8.0.1",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "5.3.3",
    "solid-js": "^1.8",
    "date-fns": "^3.3",
    "rosetty": "3.0.0"
  },
  "peerDependencies": {
    "solid-js": "^1.8",
    "date-fns": "^3.3",
    "rosetty": "3.0.0"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "solid": "./dist/index.jsx",
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "type": "module",
  "browser": {},
  "typesVersions": {}
}
