{
  "version": "1.0.37",
  "license": "MIT",
  "main": "./dist/index/server.cjs",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/qlaffont/rosetty-solid.git"
  },
  "bugs": {
    "url": "https://github.com/qlaffont/rosetty-solid/issues",
    "email": "contact@qlaffont.com"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsup src/index.tsx",
    "test": "jest",
    "lint": "eslint src test",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rosetty-solid",
  "author": "Quentin Laffont",
  "module": "./dist/index/server.js",
  "size-limit": [
    {
      "path": "dist/rosetty-solid.cjs.production.min.js",
      "limit": "300 KB"
    },
    {
      "path": "dist/rosetty-solid.esm.js",
      "limit": "200 KB"
    }
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "babelConfig": {
          "presets": [
            "babel-preset-solid",
            "@babel/preset-env"
          ]
        }
      }
    },
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "solid-js/web": "<rootDir>/node_modules/solid-js/web/dist/web.cjs",
      "solid-js": "<rootDir>/node_modules/solid-js/dist/solid.cjs"
    }
  },
  "resolutions": {
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "11.0.0",
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/jest-dom": "6.1.4",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "babel-jest": "^29.6.4",
    "babel-preset-solid": "^1.7.7",
    "dayjs": "^1.11.9",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "np": "8.0.4",
    "prettier": "3.1.0",
    "size-limit": "11.0.0",
    "solid-jest": "^0.2.0",
    "ts-jest": "^29.1.0",
    "tslib": "2.6.2",
    "tsup": "^8.0.0",
    "tsup-preset-solid": "^2.1.0",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "solid-js": "^1.7.11"
  },
  "dependencies": {
    "rosetty": "^2.0.0"
  },
  "types": "./dist/index/index.d.ts",
  "browser": {
    "./dist/index/server.js": "./dist/index/index.js",
    "./dist/index/server.cjs": "./dist/index/index.cjs"
  },
  "exports": {
    ".": {
      "worker": {
        "solid": "./dist/index/server.jsx",
        "import": {
          "types": "./dist/index/index.d.ts",
          "default": "./dist/index/server.js"
        },
        "require": {
          "types": "./dist/index/index.d.cts",
          "default": "./dist/index/server.cjs"
        }
      },
      "browser": {
        "solid": {
          "development": "./dist/index/dev.jsx",
          "import": "./dist/index/index.jsx"
        },
        "development": {
          "import": {
            "types": "./dist/index/index.d.ts",
            "default": "./dist/index/dev.js"
          },
          "require": {
            "types": "./dist/index/index.d.cts",
            "default": "./dist/index/dev.cjs"
          }
        },
        "import": {
          "types": "./dist/index/index.d.ts",
          "default": "./dist/index/index.js"
        },
        "require": {
          "types": "./dist/index/index.d.cts",
          "default": "./dist/index/index.cjs"
        }
      },
      "deno": {
        "solid": "./dist/index/server.jsx",
        "import": {
          "types": "./dist/index/index.d.ts",
          "default": "./dist/index/server.js"
        },
        "require": {
          "types": "./dist/index/index.d.cts",
          "default": "./dist/index/server.cjs"
        }
      },
      "node": {
        "solid": "./dist/index/server.jsx",
        "import": {
          "types": "./dist/index/index.d.ts",
          "default": "./dist/index/server.js"
        },
        "require": {
          "types": "./dist/index/index.d.cts",
          "default": "./dist/index/server.cjs"
        }
      },
      "solid": {
        "development": "./dist/index/dev.jsx",
        "import": "./dist/index/index.jsx"
      },
      "development": {
        "import": {
          "types": "./dist/index/index.d.ts",
          "default": "./dist/index/dev.js"
        },
        "require": {
          "types": "./dist/index/index.d.cts",
          "default": "./dist/index/dev.cjs"
        }
      },
      "import": {
        "types": "./dist/index/index.d.ts",
        "default": "./dist/index/index.js"
      },
      "require": {
        "types": "./dist/index/index.d.cts",
        "default": "./dist/index/index.cjs"
      }
    },
    "./additional": {
      "development": {
        "import": {
          "types": "./dist/additional/index.d.ts",
          "default": "./dist/additional/dev.js"
        },
        "require": {
          "types": "./dist/additional/index.d.cts",
          "default": "./dist/additional/dev.cjs"
        }
      },
      "import": {
        "types": "./dist/additional/index.d.ts",
        "default": "./dist/additional/index.js"
      },
      "require": {
        "types": "./dist/additional/index.d.cts",
        "default": "./dist/additional/index.cjs"
      }
    },
    "./shared": {
      "import": {
        "types": "./dist/shared/index.d.ts",
        "default": "./dist/shared/index.js"
      },
      "require": {
        "types": "./dist/shared/index.d.cts",
        "default": "./dist/shared/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "additional": [
        "./dist/additional/index.d.ts"
      ],
      "shared": [
        "./dist/shared/index.d.ts"
      ]
    }
  },
  "type": "module"
}
